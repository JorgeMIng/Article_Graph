@prefix mod: <https://w3id.org/mod#> .
@prefix ns: <http://open_science.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns: a owl:Ontology ;
    mod:createdWith <https://chowlk.linkeddata.es/> .

ns:abstract a owl:DatatypeProperty ;
    rdfs:label "abstract" ;
    rdfs:domain ns:Paper ;
    rdfs:range xsd:string .

ns:acknowledges a owl:ObjectProperty ;
    rdfs:label "acknowledges" ;
    rdfs:domain ns:Paper ;
    rdfs:range ns:Organization,
        ns:Project .

ns:author a owl:ObjectProperty ;
    rdfs:label "author" ;
    rdfs:domain ns:Person ;
    rdfs:range ns:Paper .

ns:belongs_to_topic a owl:ObjectProperty ;
    rdfs:label "belongs_to_topic" ;
    rdfs:domain ns:Paper ;
    rdfs:range ns:TopicBelonging .

ns:degree a owl:DatatypeProperty ;
    rdfs:label "degree" ;
    rdfs:domain ns:Similarity,
        ns:TopicBelonging ;
    rdfs:range xsd:float .

ns:email a owl:DatatypeProperty ;
    rdfs:label "email" ;
    rdfs:domain ns:Person .

ns:icon a owl:DatatypeProperty ;
    rdfs:label "icon" ;
    rdfs:domain ns:Organization ;
    rdfs:range xsd:anyURI .

ns:keyword a owl:DatatypeProperty ;
    rdfs:label "keyword" ;
    rdfs:domain ns:Topic ;
    rdfs:range xsd:string .

ns:last_name a owl:DatatypeProperty ;
    rdfs:label "last_name" ;
    rdfs:domain ns:Person ;
    rdfs:range xsd:string .

ns:latitude a owl:DatatypeProperty ;
    rdfs:label "latitude" ;
    rdfs:domain ns:Organization ;
    rdfs:range xsd:float .

ns:longitude a owl:DatatypeProperty ;
    rdfs:label "longitude" ;
    rdfs:domain ns:Organization ;
    rdfs:range xsd:float .

ns:member a owl:ObjectProperty ;
    rdfs:label "member" ;
    rdfs:domain ns:Person ;
    rdfs:range ns:Organization .

ns:name a owl:DatatypeProperty ;
    rdfs:label "name" ;
    rdfs:domain ns:Organization,
        ns:Person,
        ns:Project ;
    rdfs:range xsd:string .

ns:project_federal_id a owl:DatatypeProperty ;
    rdfs:label "project_federal_id" ;
    rdfs:domain ns:Project ;
    rdfs:range xsd:string .

ns:release_date a owl:DatatypeProperty ;
    rdfs:label "release_date" ;
    rdfs:domain ns:Paper ;
    rdfs:range xsd:date .

ns:similar_from a owl:ObjectProperty ;
    rdfs:label "similar_from" ;
    rdfs:domain ns:Similarity ;
    rdfs:range ns:Paper .

ns:similar_to a owl:ObjectProperty ;
    rdfs:label "similar_to" ;
    rdfs:domain ns:Paper ;
    rdfs:range ns:Similarity .

ns:title a owl:DatatypeProperty ;
    rdfs:label "title" ;
    rdfs:domain ns:Paper ;
    rdfs:range xsd:string .

ns:topic a owl:ObjectProperty ;
    rdfs:label "topic" ;
    rdfs:domain ns:TopicBelonging ;
    rdfs:range ns:Topic .

rdfs:label a owl:DatatypeProperty ;
    rdfs:label "label" ;
    rdfs:domain ns:Organization,
        ns:Paper,
        ns:Person,
        ns:Project,
        ns:Similarity,
        ns:Topic,
        ns:TopicBelonging ;
    rdfs:range xsd:string .

owl:sameAs a owl:DatatypeProperty ;
    rdfs:label "same as" ;
    rdfs:domain ns:Organization,
        ns:Person ;
    rdfs:range xsd:anyURI .

ns:Topic a owl:Class ;
    rdfs:label "Topic" .

ns:Project a owl:Class ;
    rdfs:label "Project" .

ns:Similarity a owl:Class ;
    rdfs:label "Similarity" .

ns:TopicBelonging a owl:Class ;
    rdfs:label "Topic Belonging" .

ns:Person a owl:Class ;
    rdfs:label "Person" .

ns:Organization a owl:Class ;
    rdfs:label "Organization" .

ns:Paper a owl:Class ;
    rdfs:label "Paper" .

